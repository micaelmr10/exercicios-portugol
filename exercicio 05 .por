programa {
  funcao inicio() {
  inteiro numero
   inteiro multiplo_de
   multiplo_de = 5

   escreva("digite o numero: ")
   leia(numero)
   se(numero % multiplo_de==0){
    escreva("o numero iformado é multiplo de 5: ")
   }
   senao{
    escreva("o numero informado nao é multiplo de 5")
   }
  }
}