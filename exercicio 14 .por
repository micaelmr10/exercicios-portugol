programa {
  funcao inicio() {

   caracter letra

   escreva("digite a letra correspondente ao genero (F/M): ")
   leia(letra)

  se (letra == "F" ou  letra =="f"){
    escreva("F - Feminino ")
   }
   senao se (letra =="M") {
      escreva("M - Masculino")
   }
   senao{
      escreva("Sexo invalido")
   }
   
   }
  }
   
  






   
  

